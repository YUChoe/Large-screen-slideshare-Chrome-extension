{
   "browser_action": {
      "default_icon": "slideshare.png"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "large_screen_slideshare.js" ],
      "matches": [ "http://www.slideshare.net/*" ]
   } ],
   "description": "This plusin will enlarge slides for slideshare.com",
   "icons": {
      "128": "slideshare.png",
      "16": "slideshare16.png",
      "48": "slideshare48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Large screen slideshare Plugin",
   "permissions": [ "activeTab" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.9",
   "web_accessible_resources": [ "large_screen_slideshare.css" ]
}
